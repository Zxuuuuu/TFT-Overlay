    <!--Phantom Dancer-->
    <s:String x:Key="PhantomDancer" xml:space="preserve">Widmowy Tancerz</s:String>
    <s:String x:Key="PhantomDancer1" xml:space="preserve">&#10;Posiadacz unika wszystkie Trafienia Krytyczne.</s:String>
    <!--Rabadon's Deathcap-->
    <s:String x:Key="RabadonsDeathcap" xml:space="preserve">Zabójczy Kapelusz Rabadona</s:String>
    <s:String x:Key="RabadonsDeathcap1" xml:space="preserve">&#10;+50% Mocy Umiejętności.</s:String>
    <!--Rapid Firecannon-->
    <s:String x:Key="RapidFirecannon" xml:space="preserve">Ognista Armata</s:String>
    <s:String x:Key="RapidFirecannon1" xml:space="preserve">&#10;Ataki posiadacza nie mogą zostać uniknięte. Zasięg ataku jest podwojony.</s:String>
    <!--Red Buff-->
    <s:String x:Key="RedBuff" xml:space="preserve">Czerwone Wzmocnienie</s:String>
    <s:String x:Key="RedBuff1" xml:space="preserve">&#10;Ataki palą za 2.5% maksymalnego zdrowia i wyłączają leczenie.</s:String>
    <!--Redemption-->
    <s:String x:Key="Redemption" xml:space="preserve">Odkupienie</s:String>
    <s:String x:Key="Redemption1" xml:space="preserve">&#10;Po śmierci leczy pobliskich sojuszników, przywracając im 1000 zdrowia.</s:String>
    <!--Seraph's Embrace-->
    <s:String x:Key="SeraphsEmbrace" xml:space="preserve">Uścisk Serafina</s:String>
    <s:String x:Key="SeraphsEmbrace1" xml:space="preserve">&#10;Odnawia 20 many za każdym razem, gdy używana jest umiejętność.</s:String>
    <!--Spear of Shojin-->
    <s:String x:Key="SpearofShojin" xml:space="preserve">Włócznia Shojin</s:String>
    <s:String x:Key="SpearofShojin1" xml:space="preserve">&#10;Po użyciu umiejętność, posiadacz odnawia 15% many po każdym ataku.</s:String>
    <!--Spear of Shojin-->
    <s:String x:Key="StatikkShiv" xml:space="preserve">Kosa Statikka</s:String>
    <s:String x:Key="StatikkShiv1" xml:space="preserve">&#10;Co każdy 3. atak zadaje 100 magicznych obrażeń.</s:String>
    <!--Sword Breaker-->
    <s:String x:Key="SwordBreaker" xml:space="preserve">Łamimiecz</s:String>
    <s:String x:Key="SwordBreaker1" xml:space="preserve">&#10;Atak ma szansę na rozbrojenie.</s:String>
    <!--Sword of the Divine-->
    <s:String x:Key="SwordDivine" xml:space="preserve">Boski Miecz</s:String>
    <s:String x:Key="SwordDivine1" xml:space="preserve">&#10;Co każdą sekundę, posiadacz ma 5% szans na zdobycie 100% szans na Trafienie Krytyczne.</s:String>
    <!--Thornmail-->
    <s:String x:Key="Thornmail" xml:space="preserve">Kolczasta Kolczuga</s:String>
    <s:String x:Key="Thornmail1" xml:space="preserve">&#10;Odbija 35% odtrzymanych fizycznych obrażeń.</s:String>
    <!--Titanic Hydra-->
    <s:String x:Key="TitanicHydra" xml:space="preserve">Kolosalna Hydra</s:String>
    <s:String x:Key="TitanicHydra1" xml:space="preserve">&#10;Ataki zadają 10% maksymalnego zdrowia posiadacza jako obrażenia rozpryskowe.</s:String>
    <!--Warmog's Armor-->
    <s:String x:Key="WarmogsArmor" xml:space="preserve">Plemienna Zbroja</s:String>
    <s:String x:Key="WarmogsArmor1" xml:space="preserve">&#10;Posiadacz odnawia 3% maksymalnego zdrowia co sekundę.</s:String>
    <!--Zeke's Herald-->
    <s:String x:Key="ZekesHerald" xml:space="preserve">Herold Zeke'a</s:String>
    <s:String x:Key="ZekesHerald1" xml:space="preserve">&#10;Sąsiadujący sojucznicy otrzymują +10% prędkości ataku.</s:String>
    <!--Zephyr-->
    <s:String x:Key="Zephyr" xml:space="preserve">Zefir</s:String>
    <s:String x:Key="Zephyr1" xml:space="preserve">&#10;Na początku walki, wyklucza jednego przeciwnika na 5 sekund.</s:String>

