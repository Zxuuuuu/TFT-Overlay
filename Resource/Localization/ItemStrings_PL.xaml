    <!-- Demons -->
    <s:String x:Key="ClassOrigins_Demons" xml:space="preserve">Demon</s:String>
    <s:String x:Key="ClassOrigins_Demons_Describe1" xml:space="preserve">40% (2), 60% (4), 80% (6) szans podczas ataku na spalenie całej</s:String>
    <s:String x:Key="ClassOrigins_Demons_Describe2" xml:space="preserve">aktualnej many celu</s:String>
    <s:String x:Key="ClassOrigins_Demons_Describe3" xml:space="preserve">, zadając</s:String>
    <s:String x:Key="ClassOrigins_Demons_Describe4" xml:space="preserve">nieuchronne obrażenia</s:String>
    <s:String x:Key="ClassOrigins_Demons_Describe5" xml:space="preserve">równe spalonej manie.</s:String>

